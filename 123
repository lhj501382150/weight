
CREATE TABLE SaleOrders(
	Id int AUTO_INCREMENT NOT null,
	Code varchar(60) NOT null,
	CustomerCode varchar(30) ,
	CustomerName varchar(60) ,
	VendorCode varchar(30) ,
	Vendor varchar(60) ,
	MaterialID varchar(30) ,
	MaterialCode varchar(30) ,
	MaterialName varchar(30) ,
	SpecificationsModel varchar(30) ,
	Total decimal(19, 5) ,
	SendCount decimal(19, 5) ,
	Count decimal(19, 5) ,
	CreateTime varchar(30) ,
	SaveTime varchar(30) ,
	ReturnSign int ,
	IsComplete int ,
	Operator varchar(30) ,
	OperatorCode varchar(30) ,
	BizType varchar(30) ,
	C1 varchar(30) ,
	C2 varchar(30) ,
	Adjust int ,
	EnableFlag int ,
	Warning decimal(19, 5) ,
	PRIMARY KEY Id
)